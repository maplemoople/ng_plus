//collectedMenu(collected)
function collectedMenu(collected){
		switch(collected){
				case 0:
				return "";
				
				case 1:
				return "O";
				
				case 2:
				return "OO";
				
				case 3:
				return "OOO";
				
				case 4:
				return "OOOO";
				
				case 5:
				return "OOOOO";
				
				default:
				return "";
			}
}